<?xml version='1.0' encoding='utf-8'?>
<widget id="com.pettails.android.AMRoche" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="EstimoteBeacons">
        <param name="android-package" value="com.oracle.mx.ux.cordova.estimotebeacons.EstimoteBeacons" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
    </feature>
    <name>pettails</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <icon src="img/dog-chasing-tail.jpg" />
    <preference name="Orientation" value="portrait" />
    <preference name="SplashScreen" value="dog_chasing_tail_splash" />
    <preference name="SplashScreenDelay" value="10000" />
    <splash height="629" src="www/img/dog-chasing-tail-splash.png" width="473" />
</widget>
